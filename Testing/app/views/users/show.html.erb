<div class="row">
	<div class="span6 offset3">
		<div class="well">
			<%= image_tag @pin.image %>
	    <p>
	     	<%= @pin.description %>
	    </p>
	    <p>
	     	<%= @pin.created_at %>
	    </p>
	     <p>
	     	<%= @pin.user.name %>
	    </p>
	    <%= link_to @pin.user.name, "/users/show"+@pin.user.id.to_s %>
	    <%= link_to 'Edit User', edit_pin_path(@pin) %> |
			<%= link_to 'Back', pins_path %>
    </div>
  </div>
</div>
